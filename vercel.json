{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/locales/(.*)",
      "dest": "/locales/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/json; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
